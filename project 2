// Student class
class Student {
  String name;
  int age;
  String gradeLevel;

  Student(this.name, this.age, this.gradeLevel);

  void printStudentInfo() {
    print('Student: Name - $name, Age - $age, Grade Level - $gradeLevel');
  }
}

// Teacher class
class Teacher {
  String name;
  int age;
  String subject;

  Teacher(this.name, this.age, this.subject);

  void printTeacherInfo() {
    print('Teacher: Name - $name, Age - $age, Subject - $subject');
  }
}

// School class
class School {
  void run() {
    // Create instances of Student and Teacher
    var student = Student('Alice', 15, '10th');
    var teacher = Teacher('Mr. Smith', 35, 'Mathematics');

    // Call methods to print information
    student.printStudentInfo();
    teacher.printTeacherInfo();
  }
}

// Main function
void main() {
  // Create an instance of the School class and run the program
  var school = School();
  school.run();
}
